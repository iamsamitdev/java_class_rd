package com.itgenius;

public class JavaMath {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(Math.PI);
		System.out.println(Math.E);
		System.out.println(Math.exp(10));
		System.out.println(Math.pow(2, 3));
		System.out.println(Math.max(9, 12));
		System.out.println(Math.min(9, 12));
		System.out.println(Math.floor(27.564));
		System.out.println(Math.ceil(27.564));
		System.out.println(Math.round(27.564));
		System.out.println(String.format("%.0f",Math.random()*10000));
	}

}
